{
  "name": "further-review",
  "version": "1.0.0-alpha.1",
  "description": "Pull request process enforcer.",
  "keyword": [
    "pull request", "pr", "glob", "lgtm", "github",
    "webhook", "process"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/paultyng/further-review.git"
  },
  "main": "main.js",
  "dependencies": {
    "apex.js": "^2.0.0",
    "aws-sdk": "^2.5.2",
    "axios": "^0.14.0",
    "babel-polyfill": "^6.9.1",
    "babel-register": "^6.11.6",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.14.0",
    "js-yaml": "^3.6.1",
    "minimatch": "^3.0.2",
    "nconf": "github:paultyng/nconf#browserify",
    "normalize-package-data": "^2.3.5"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.8.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.11.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "coveralls": "^2.11.12",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "nyc": "^8.1.0",
    "sinon": "^1.17.5"
  },
  "scripts": {
    "start": "node ./main.js",
    "test": "eslint . && nyc ava",
    "coverage": "nyc --reporter=text-lcov ava > ./coverage/lcov.info; which lcov && lcov -l ./coverage/lcov.info",
    "test-hook": "curl -X POST -d @event.json -H 'Content-Type: application/json' http://localhost:3000/github-webhook"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
